{
  "name": "consolidator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build-dev": "ng build --prod --configuration=dev --base-href /app/consolidator/v1/",
    "build-qa": "ng build --prod --configuration=uat --base-href /app/consolidator/v1/",
    "build-stage": "ng build --prod --configuration=stage --base-href /app/consolidator/v1/",
    "build-prod": "ng build --prod --configuration=prod --base-href /app/consolidator/v1/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "ngcc"
  },
  "private": true,
